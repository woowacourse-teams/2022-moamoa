{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "auto",

  "importOrder": [
    "^@emotion",
    "^@assets/(.*)$",
    "^@constants/(.*)$",
    "^@custom-types/(.*)$",
    "^@styles/(.*)$",
    "^@api/(.*)$",
    "^@context/(.*)$",
    "^@layout/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
